<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/relativeLayoutBoxTrackingsWhReceipt_inflate"
    android:layout_height="wrap_content"
    android:layout_alignParentTop="true"
    android:layout_toRightOf="@+id/relativeLayoutPopupAddItemLO_inflate"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content">

    <TableLayout
        android:id="@+id/tableLayoutAddTrackingBoxWhLO_inflate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp" >

        <TableRow
            android:id="@+id/tableAddTrackingLORow1_inflate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/textViewTrackingItemLOLabel_inflate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Tracking"
                android:textSize="19sp"
                android:textStyle="bold" />
        </TableRow>

        <TableRow
            android:id="@+id/tableAddTrackingLORow2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <EditText
                android:id="@+id/editTextDialogItemTrackingLO_inflate"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_gravity="top|left"
                android:ems="10"
                android:hint="Tracking"
                 />
        </TableRow>
        <TableRow
            android:id="@+id/tableAddTrackingLORow3_inflate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp" >

            <Button
                android:id="@+id/buttonDialogAddEachTrackingLO_inflate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_span="2"
                android:text="Add"
                android:textSize="18sp" />

        </TableRow>
    </TableLayout>

    <!--  box show each tracking added  -->

    <TableLayout
        android:id="@+id/tableTrackingsItemWhReceipt_inflate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/tableLayoutAddTrackingBoxWhLO_inflate"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="10dp"
        android:layout_toRightOf="@+id/tableLayoutAddTrackingBoxWhLO_inflate"
        android:background="#D8D8D8" >

        <TableRow
            android:id="@+id/tableListTrackingsWhLORow1_inflate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/textViewTitleTableTrackingsItemLOLabel_inflate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:padding="5dp"
                android:text="Tracking"
                android:textSize="19sp"
                android:textStyle="bold" />

        </TableRow>

        <TableRow
            android:id="@+id/tableListTrackingsWhLORow2_inflate"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <FrameLayout
                android:id="@+id/LineaTotalesTrackings"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@android:color/holo_blue_light" >
            </FrameLayout>
        </TableRow>
    </TableLayout>


</RelativeLayout>
